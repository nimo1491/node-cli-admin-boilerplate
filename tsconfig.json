{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es6",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "strict": true,
    "outDir": "./built"
  },
  "exclude": [
    "node_modules",
    "dist",
    "built"
  ],
  "compileOnSave": false
}
